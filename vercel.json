{
  "installCommand": "npm ci",
  "buildCommand": "npm run type-check && if [ \"$VERCEL_ENV\" = \"production\" ]; then npm run build:production; else npm run build:staging; fi",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/n8n-webhook",
      "destination": "https://aztec.app.n8n.cloud/webhook/:env"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

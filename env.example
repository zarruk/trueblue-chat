# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration
VITE_APP_NAME=TrueBlue Chat Management
VITE_APP_VERSION=0.1.0
VITE_APP_ENV=development

# n8n Webhook Configuration
VITE_N8N_WEBHOOK_URL=your_n8n_webhook_url_here

# App URL for redirects
VITE_APP_URL=http://localhost:3000/

# Feature Flags
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_ANALYTICS=false

# WATI Image Authentication Tokens
# These tokens should be obtained from your WATI dashboard
# DO NOT hardcode these values in your source code
VITE_WATI_DEV_TOKEN=your_wati_dev_token_here
VITE_WATI_PROD_TOKEN=your_wati_prod_token_here
